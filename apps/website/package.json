{
  "name": "@chatgame/website",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "prisma generate --schema ../../prisma/schema.prisma && nuxt build",
    "dev": "prisma generate --schema ../../prisma/schema.prisma && nuxt dev",
    "preview": "nuxt preview",
    "clean": "rm -rf dist",
    "clean:modules": "rm -rf .nuxt .output node_modules",
    "typecheck": "prisma generate --schema ../../prisma/schema.prisma && nuxt typecheck",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@chat-game/game": "workspace:*",
    "@chat-game/locale": "workspace:*",
    "@chat-game/types": "workspace:*",
    "@chatgame/database": "workspace:*",
    "@chatgame/stream-journey": "catalog:integrations",
    "@donation-alerts/api": "catalog:integrations",
    "@donation-alerts/auth": "catalog:integrations",
    "@donation-alerts/events": "catalog:integrations",
    "@number-flow/vue": "catalog:integrations",
    "@nuxt/ui": "catalog:integrations",
    "@paralleldrive/cuid2": "catalog:integrations",
    "@prisma/client": "6.2.1",
    "@telegram-apps/init-data-node": "catalog:integrations",
    "@twurple/api": "catalog:integrations",
    "@twurple/auth": "catalog:integrations",
    "@twurple/chat": "catalog:integrations",
    "@twurple/easy-bot": "catalog:integrations",
    "@twurple/eventsub-ws": "catalog:integrations",
    "@vueuse/components": "catalog:integrations",
    "@vueuse/core": "catalog:integrations",
    "@vueuse/nuxt": "catalog:integrations",
    "grammy": "catalog:integrations",
    "zod": "catalog:integrations"
  },
  "devDependencies": {
    "@d-fischer/typed-event-emitter": "3.3.3",
    "@iconify-json/lucide": "catalog:dev",
    "@iconify-json/simple-icons": "catalog:dev",
    "@nuxt/devtools": "catalog:dev",
    "@nuxtjs/i18n": "catalog:dev",
    "@types/node": "catalog:types",
    "nuxt": "catalog:dev",
    "nuxt-auth-utils": "catalog:dev",
    "prisma": "6.2.1",
    "vue-tsc": "catalog:dev"
  }
}
